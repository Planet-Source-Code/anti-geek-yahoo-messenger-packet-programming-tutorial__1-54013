\par
\pard\qc\cf4 Public Function HextoAscii(inputstr As String) As String\par
\cf5 Dim spilter As Variant, i As Integer, finnal As String\par
If InStr(1, inputstr, " ") <> 0 Then\par
spilter = Split(inputstr, " ")\par
For i = 0 To UBound(spilter)\par
finnal = finnal & Chr(Val("&H" & spilter(i)))\par
Next i\par
HextoAscii = finnal\par
ElseIf Len(inputstr) = 2 Then\par
finnal = Chr(Val("&H" & inputstr))\par
HextoAscii = finnal\par
End If\cf4\par
End Function\par
\cf3\par
\cf4 Public Function Asciitohex(inputstr As String) As String\par
\cf5 On Error Resume Next\par
Dim spilter As Variant, i As Integer, finnal As String\par
For i = 1 To Len(inputstr)\par
finnal = finnal & Hex(Asc(Mid(inputstr, i, 1))) & " "\par
Next i\par
Asciitohex = Mid(finnal, 1, Len(finnal) - 1)\cf4\par
End Function\par
\pard\cf0\par
